{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "description": "A bar chart showing the US population distribution of age groups in 2000.",
  "data": {"url": "https://raw.githubusercontent.com/divskivs/tableau1/master/losses2015_transformed.csv", "format": {"type": "csv"}},
  "mark": "bar",
  "encoding": {
    "y": {"field": "State_Abv", "type": "nominal","sort": {"op": "sum", "field": "Amount", "order":"ascending"}},
    "x": {
      "aggregate": "sum",
      "field": "Amount",
      "type": "quantitative"
    }
  },
  "config": {"axisY": {"minExtent": 30}}
}